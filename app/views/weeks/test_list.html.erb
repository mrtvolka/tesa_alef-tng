<div id="faux-background" class="mobile-content"></div>
<nav class="without-icons not-zoomable">
  <%= render 'shared/user_element' %>
  <div class="mobile-content">
    <h2><%= @setup.name %></h2>
    <a class="user-element-cta"><i class="nav-right-icons icon-menu"></i></a>
  </div>
</nav>
<hr class="nav-offset not-zoomable">


<div class="mobile-content main">
  <% if !@week_tests.any? %>
      Žiadne odovzdané testy!
  <% end %>
  <% @week_tests.each do |w| %>
      <a href="<%= url_for controller: 'questions', action: 'access_answers', week_number: w.number %>"
         class="card <%= 'week-current' if w.actual? %>">
        <h3 class="card-header"><%= w.number %>. týždeň - TEST<%= '(aktuálny)' if w.actual? %><date class="week-date"><%=
            w.start_at.strftime("%-d.%-m.")  %> - <%= w.end_at.strftime("%-d.%-m.%Y") %></date></h3>
      </a>
  <% end %>
</div>